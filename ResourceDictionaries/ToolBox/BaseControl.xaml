<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:base="clr-namespace:BoardDesigner.Base">
    <base:ToolBox x:Key="BaseControlToolBox"
               DefaultItemSize="25,25">
        <ItemsControl.Items>
            <Image IsHitTestVisible="False" x:Name="Label"
                   Stretch="Fill"
                   Source="\Resources\Images\T 16X16.png" />
            <Image IsHitTestVisible="False" x:Name="Image"
                   Stretch="Fill"
                   Source="\Resources\Images\image 24X24.png" />
            <Image IsHitTestVisible="False" x:Name="GIF"
                   Stretch="Fill"
                   Source="\Resources\Images\GIF 24X24.png" />
            <Image IsHitTestVisible="False" x:Name="Video"
                   Stretch="Fill"
                   Source="\Resources\Images\Video 16X16.png" />
            <Image IsHitTestVisible="False" x:Name="WebBrowser"
                   Stretch="Fill"   
                   Source="\Resources\Images\chrome 24X24.png" />
        </ItemsControl.Items>
    </base:ToolBox>
</ResourceDictionary>